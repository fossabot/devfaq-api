{
  "type": "sqlite",
  "database": "fefaq.sqlite",
  "synchronize": true,
  "logging": true,
  "entities": [
    "app/entity/**/*.ts"
  ],
  "migrations": [
    "app/migration/**/*.ts"
  ],
  "subscribers": [
    "app/subscriber/**/*.ts"
  ],
  "cli": {
    "entitiesDir": "app/entity",
    "migrationsDir": "app/migration",
    "subscribersDir": "app/subscriber"
  }
}
